  "source": "pragma solidity ^0.5.8;\n\ncontract HelloWorld {\n    address payable owner;\n    string greeting = \"Hello World\";\n    // Constructor function\n    constructor() public {\n        owner = msg.sender;\n    }\n\n    function greet() view public returns (string memory x) {\n        return greeting;\n    }\n\n    function kill() public {\n        require(owner == msg.sender);\n        selfdestruct(owner);\n    }\n}",
